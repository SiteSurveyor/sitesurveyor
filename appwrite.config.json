    "tablesDB": [
        {
            "$id": "sitesurveyor",
            "name": "SiteSurveyor",
            "enabled": true
        }
    ],
    "tables": [
        {
            "$id": "posts",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "sitesurveyor",
            "name": "Posts",
            "enabled": true,
            "rowSecurity": false,
            "columns": [
                {
                    "key": "title",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "slug",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "excerpt",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 500,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "content",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100000,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "coverImage",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 500,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "authorId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "authorName",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "status",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "draft",
                        "published"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "publishedAt",
                    "type": "datetime",
                    "required": false,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "tags",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 100,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "readTime",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 1,
                    "max": 120,
                    "default": null
                }
            ],
            "indexes": []
        },
        {
            "$id": "newsletter",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")"
            ],
            "databaseId": "sitesurveyor",
            "name": "Newsletter Subscribers",
            "enabled": true,
            "rowSecurity": false,
            "columns": [
                {
                    "key": "email",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "format": "email",
                    "default": null
                },
                {
                    "key": "subscribedAt",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "status",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 20,
                    "default": null,
                    "encrypt": false
                }
            ],
            "indexes": []
        }
